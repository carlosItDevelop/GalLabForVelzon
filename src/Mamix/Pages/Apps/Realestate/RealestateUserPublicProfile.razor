   public WMBSCInitialSettings? ResponsiveConfigurations;  
    protected override void OnInitialized() {
        WMBSCSettings breakpoint768Settings = new WMBSCSettings
        {
            slidesToShow = 1,
            arrows = false,
            dots = false,
            autoplay = true,
            autoplaySpeed = 1500,
        };
        WMBSCResponsiveSettings breakpoint768Responsive = new WMBSCResponsiveSettings
        {
            breakpoint = 768,
            settings = breakpoint768Settings
        };
        WMBSCSettings breakpoint1024Settings = new WMBSCSettings
        {
            slidesToShow = 2,
            arrows = true,
            dots = false,
            autoplay = true,
            autoplaySpeed = 1500,
        };
        WMBSCResponsiveSettings breakpoint1024Responsive = new WMBSCResponsiveSettings
        {
            breakpoint = 1024,
            settings = breakpoint1024Settings
        };
        WMBSCSettings breakpoint1400Settings = new WMBSCSettings
        {
            slidesToShow = 2,
            arrows = true,
            dots = false,
            autoplay = true,
            autoplaySpeed = 1500,
        };
        WMBSCResponsiveSettings breakpoint1400Responsive = new WMBSCResponsiveSettings
        {
            breakpoint = 1400,
            settings = breakpoint1400Settings
        };
        List<WMBSCResponsiveSettings> responsiveSettingsList = new List<WMBSCResponsiveSettings>();
        responsiveSettingsList.Add(breakpoint768Responsive);
        responsiveSettingsList.Add(breakpoint1024Responsive);
        responsiveSettingsList.Add(breakpoint1400Responsive);

        ResponsiveConfigurations = new WMBSCInitialSettings
        {
            arrows = true,
            dots = false,
            slidesToShow = 3,
            slidesToScroll = 1,
            infinite = true,
            autoplay = true,
            autoplaySpeed = 1500,
            responsive = responsiveSettingsList,
        };

    }

    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            await JSRuntime.InvokeVoidAsync("window.interop.initializeTooltips");
        }
    }

